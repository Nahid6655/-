#include <iostream>

using namespace std;


int main()
{
    setlocale(LC_ALL, "RU");

    float dl;
    float sh;
    cout << "Ведите длину прямоугольника" << endl;
    cin >> dl;
    cout << "Ведите ширину прямоугольника " << endl;
    cin >> sh;
    cout << " Площадь равна: " << dl * sh << endl;

    return 0;
}
